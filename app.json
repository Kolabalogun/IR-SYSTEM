{
  "expo": {
    "name": "IR SYSTEM",
    "slug": "IRSYSTEM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/al.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/al.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F8FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/al.png",
        "backgroundColor": "#F0F8FF"
      },
      "package": "com.nacosploy.alertApp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/al.png"
    }
  }
}
